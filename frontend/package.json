{
  "name": "rsd-frontend",
  "version": "2.26.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "dev:docker": "NODE_ENV=docker next dev  # backup: NODE_ENV=docker NODE_OPTIONS='--inspect=0.0.0.0:9229' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:coverage": "jest --coverage",
    "test:coveragememlimit": "jest --coverage --workerIdleMemoryLimit=0.1 --maxWorkers=2",
    "test:memory": "node --expose-gc ./node_modules/.bin/jest --logHeapUsage",
    "test:inspect": "node --inspect-brk --expose-gc ./node_modules/.bin/jest --logHeapUsage",
    "test:memlimit": "jest --workerIdleMemoryLimit=0.1 --maxWorkers=2",
    "knip:files": "knip --include files",
    "knip:types": "knip --include types",
    "knip:exports": "knip --include exports",
    "knip": "knip"
  },
  "dependencies": {
    "@dnd-kit/core": "6.0.8",
    "@dnd-kit/modifiers": "6.0.1",
    "@dnd-kit/sortable": "7.0.2",
    "@emotion/react": "11.10.6",
    "@emotion/server": "11.10.0",
    "@emotion/styled": "11.10.6",
    "@mui/icons-material": "5.10.16",
    "@mui/material": "5.10.17",
    "@tailwindcss/typography": "0.5.16",
    "aos": "2.3.4",
    "cookie": "1.0.2",
    "d3": "7.9.0",
    "dompurify": "3.2.3",
    "jsonwebtoken": "9.0.2",
    "next": "15.1.4",
    "nprogress": "0.2.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.54.2",
    "react-markdown": "8.0.7",
    "remark-breaks": "3.0.2",
    "remark-gfm": "3.0.1",
    "sharp": "0.33.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.2.0",
    "@svgr/webpack": "8.1.0",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.1.0",
    "@types/aos": "3.0.7",
    "@types/d3": "7.4.3",
    "@types/jest": "29.5.14",
    "@types/jsonwebtoken": "9.0.7",
    "@types/node": "20.3.3",
    "@types/nprogress": "0.2.3",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "autoprefixer": "10.4.20",
    "eslint": "9.17.0",
    "eslint-config-next": "15.1.4",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "knip": "5.42.0",
    "postcss": "8.4.49",
    "tailwindcss": "3.4.17",
    "typescript": "5.7.3",
    "whatwg-fetch": "3.6.20"
  }
}
